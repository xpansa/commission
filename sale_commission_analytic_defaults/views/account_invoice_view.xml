<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_account_invoice_form" model="ir.ui.view">
            <field name="name">account.invoice.form</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_line']/tree//field[@name='account_analytic_id']" position="attributes">
                    <attribute name="on_change">onchange_account_analytic_id(account_analytic_id)</attribute>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
