<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_account_analytic_account_form" model="ir.ui.view">
            <field name="name">account.analytic.account.form</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_account_form" />
            <field name="arch" type="xml">
                <group name="main" position="after">
                    <label for="default_agents" string='Default Agents and Commissions' />
                    <field name="default_agents" />
                </group>
            </field>
        </record>

        <record id="view_analytic_account_agent_line_tree" model="ir.ui.view" >
            <field name="name">analytic.account.agent.line.tree</field>
            <field name="model">analytic.account.agent.line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="agent" />
                    <field name="commission" />
                </tree>
            </field>
        </record>

    </data>
</openerp>
